{
    "rabbitmq": {
        "host": "rabbit",
        "port": "5672",
        "user": "guest",
        "password": "guest"
    },
    "error_policy": {
        "ignore_all": false,
        "tolerance_number": 1
    },
    "logs": {
        "version": 1,
        "propagate": true,
        "formatters": {
            "default": {
                "format": "%(asctime)s.%(msecs)05d %(levelname)s: %(message)s",
                "datefmt": "%m/%d/%Y %H:%M:%S"
            }
        },
        "handlers": {
            "console": {
                "class": "logging.StreamHandler",
                "level": "DEBUG",
                "formatter": "default",
                "stream": "ext://sys.stdout"
            }
        },
        "loggers": {

        },
        "root": {
            "level": "DEBUG",
            "handlers": ["console"]
        }
    }
}
